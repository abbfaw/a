<?xml version="1.0" encoding="UTF-8" ?>

<odoo>

    <record id="secii_commerciaux_access" model="ir.rule">
        <field name="name">Encaissement des commerciaux</field>
        <field name="model_id" ref="account_secii.model_secii_encaissement"/>
        <field name="groups" eval="[(4, ref('secii_commerciaux'))]"/>
        <field name="perm_read" eval="1"/>
