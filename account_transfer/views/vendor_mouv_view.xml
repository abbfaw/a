<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="mouvements_inherit" model="ir.ui.view">
        <field name="name">Heritage mouvements fournisseur</field>
        <field name="model">vendor.form</field>
        <field name="inherit_id" ref="vendor_form.view_vendor_form"/>
        <field name="arch" type="xml">
            <xpath expr="//page[@name='mouvements']" position="before">
                <page name="mouvements_list" string='Liste des Mouvements'>
                    <field name="mouvements">
                        <tree>
                            <field name="vendor"/>
                            <field name="currency"/>
                            <field name="amount"/>
                        </tree>
                    </field>
                </page>
            </xpath>
        </field>
    </record>
</odoo>