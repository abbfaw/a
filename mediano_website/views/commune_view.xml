<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <!-- Vue form pour les communes -->
	<record id="address_commune_form_view" model="ir.ui.view">
        <field name="name">res.commune.form</field>
        <field name="model">res.commune</field>
        <field name="arch" type="xml">
        	<form>
        		<sheet>
                    <group>
            			<field name="name" />
<!--                        <field name="city_id" />-->
                    </group>
        		</sheet>
        	</form>
        </field>
    </record>

    <!-- Vue liste pour les communes -->
    <record id="address_commune_tree_view" model="ir.ui.view">
        <field name="name">res.commune.tree</field>
        <field name="model">res.commune</field>
        <field name="arch" type="xml">
        	<tree>
        		<field name="name" />
<!--                <field name="city_id" />-->
        	</tree>
        </field>
    </record>

</odoo>